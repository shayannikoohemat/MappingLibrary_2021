	$(MAKE) -C abs2ext -f Makefile_abs2ext.win clean
	$(MAKE) -C absor -f Makefile_absor.win clean
	$(MAKE) -C ext2abs -f Makefile_ext2abs.win clean
	$(MAKE) -C extor -f Makefile_extor.win clean
	$(MAKE) -C intor -f Makefile_intor.win clean
	$(MAKE) -C relor -f Makefile_relor.win clean
