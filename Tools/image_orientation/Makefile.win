all:
	$(MAKE) -C abs2ext -f Makefile_abs2ext.win
	$(MAKE) -C absor -f Makefile_absor.win
	$(MAKE) -C ext2abs -f Makefile_ext2abs.win
	$(MAKE) -C extor -f Makefile_extor.win
	$(MAKE) -C intor -f Makefile_intor.win
