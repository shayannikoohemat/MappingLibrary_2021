# Project: linex2
# Makefile created by Dev-C++ 4.9.9.2

GCCDIR = /usr/local/gcc342
CPP  = $(GCCDIR)/bin/g++
CC  = $(GCCDIR)/bin/gcc
WINDRES = windres.exe
RES  = 
#OBJ  = main.o ../linex/llinex.o ../linex/foerstner.o ../linex/linegrow.o ../linex/burns.o $(RES)
#LINKOBJ  = main.o ../linex/llinex.o ../linex/foerstner.o ../linex/linegrow.o ../linex/burns.o $(RES)
OBJ  = main.o 
LINKOBJ  = main.o 
LIBS =  -L"lib" -L/usr/local/lib -lhighgui -lcv -lcxcore -lcvaux -L"../../../Library/lib" -llinex_lx -lLaserScan_lx -lPhotogrammetry_lx  -lNR_lx -lEispack_lx -lLinpack_lx -lANN_lx -lnewmat_lx -lg2c   -g3 
INCS =  -I"include"  -I/usr/local/include/opencv -I"../../../Library/LaserScan/include"  -I"../../../Library/Photogrammetry/include"  -I"../../../Foreign/NR/utils" 
CXXINCS =  -I$(GCCDIR)/include -I"../../../Library/LaserScan/include"  -I"../../../Library/Photogrammetry/include"  -I"../../../Foreign/NR/utils"  -I/usr/local/include/opencv
BIN  = ../../bin/linex2_lx
CXXFLAGS = $(CXXINCS)   -traditional-cpp -g3
CFLAGS = $(INCS)   -traditional-cpp -g3
RM = rm -f

.PHONY: all all-before all-after clean clean-custom

all: all-before ../../bin/linex2_lx all-after


clean: clean-custom
	${RM} $(OBJ) $(BIN)

$(BIN): $(OBJ)
	$(CPP) $(LINKOBJ) -o "../../bin/linex2_lx" $(LIBS)

main.o: main.cpp
	$(CPP) -c main.cpp -o main.o $(CXXFLAGS)

