# Project: createpointset
# Makefile created by Dev-C++ 4.9.9.2

CPP  = g++.exe -D__DEBUG__
CC   = gcc.exe -D__DEBUG__
WINDRES = windres.exe
RES  = 
OBJ  = main.o createpointset_cpp.o $(RES)
LINKOBJ  = main.o createpointset_cpp.o $(RES)
LIBS = -L $(MAPPING_LIB_DIR) -lLaserScan -lPhotogrammetry -llas -lEispack -lLinpack -lANN -lnewmat -lgfortran   -g3 
INCS =  -I"../../../Library/LaserScan/include"  -I"../../../Library/Photogrammetry/include" 
CXXINCS = $(INCS)
BIN  = ../../bin/createpointset.exe
CXXFLAGS = $(CXXINCS)   -g3
CFLAGS = $(INCS)   -g3
RM = rm -f

.PHONY: all all-before all-after clean clean-custom

all: all-before ../../bin/createpointset.exe all-after


clean: clean-custom
	${RM} $(OBJ) $(BIN)

$(BIN): $(OBJ)
	$(CPP) $(LINKOBJ) -o "..\..\bin\createpointset.exe" $(LIBS)

main.o: main.cpp
	$(CPP) -c main.cpp -o main.o $(CXXFLAGS)

createpointset_cpp.o: createpointset_cpp.cc
	$(CPP) -c createpointset_cpp.cc -o createpointset_cpp.o $(CXXFLAGS)
